<html>
<body> 
    <h1>All Cars</h1>
    <nav>
      <a href="/cars/new">Create a New Car</a>
    </nav>
    
    <ul>
    <% for (i in cars) { %>
      <li>
        <%= cars[i].name %> 
      </li>
      <a href="/cars/<%= cars[i]._id  %>">SHOW</a>
      <a href="/cars/<%= cars[i]._id  %>/edit">EDIT</a>
      <form action="/cars/<%= cars[i]._id  %>?_method=DELETE" method="POST">
        <input type="submit" value="DELETE"/>
      </form>
      <% } %>
    </ul>
</body>
</html>